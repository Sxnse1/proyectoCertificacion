    <!-- Bootstrap 5.3 JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    
    <script>
        // Referencias a elementos del DOM
        const fileDropArea = document.getElementById('fileDropArea');
        const fileInput = document.getElementById('videoFile');
        const fileInfo = document.getElementById('fileInfo');
        const fileName = document.getElementById('fileName');
        const fileSize = document.getElementById('fileSize');
        const uploadForm = document.getElementById('uploadForm');
        const uploadProgress = document.getElementById('uploadProgress');
        const progressBar = document.getElementById('progressBar');
        const progressPercent = document.getElementById('progressPercent');
        const progressStatus = document.getElementById('progressStatus');
        const uploadStatus = document.getElementById('uploadStatus');
        const submitBtn = document.getElementById('submitBtn');

        // Configurar drag and drop
        fileDropArea.addEventListener('click', () => fileInput.click());

        fileDropArea.addEventListener('dragover', (e) => {
            e.preventDefault();
            fileDropArea.classList.add('dragover');
        });

        fileDropArea.addEventListener('dragleave', () => {
            fileDropArea.classList.remove('dragover');
        });

        fileDropArea.addEventListener('drop', (e) => {
            e.preventDefault();
            fileDropArea.classList.remove('dragover');
            
            const files = e.dataTransfer.files;
            if (files.length > 0) {
                handleFileSelection(files[0]);
            }
        });

        fileInput.addEventListener('change', (e) => {
            if (e.target.files.length > 0) {
                handleFileSelection(e.target.files[0]);
            }
        });

        function handleFileSelection(file) {
            // Validar tipo de archivo
            const allowedTypes = ['video/mp4', 'video/avi', 'video/mov', 'video/wmv', 'video/quicktime'];
            if (!allowedTypes.includes(file.type)) {
                showStatus('error', 'Formato de archivo no soportado. Usa MP4, AVI, MOV o WMV.');
                return;
            }

            // Validar tama√±o (2GB m√°ximo)
            const maxSize = 2 * 1024 * 1024 * 1024; // 2GB en bytes
            if (file.size > maxSize) {
                showStatus('error', 'El archivo es demasiado grande. M√°ximo 2GB permitido.');
                return;
            }

            // Mostrar informaci√≥n del archivo
            fileName.textContent = file.name;
            fileSize.textContent = formatFileSize(file.size);
            fileInfo.classList.add('show');
            fileDropArea.classList.add('has-file');
            
            // Actualizar texto del √°rea de drop
            fileDropArea.querySelector('.drop-content').innerHTML = `
                <i class="bi bi-check-circle text-success" style="font-size: 3rem;"></i>
                <h5 class="mt-3 mb-2 text-success">Archivo seleccionado</h5>
                <p class="text-muted mb-0">Haz clic en "Subir Video" para continuar</p>
            `;

            hideStatus();
        }

        function removeFile() {
            fileInput.value = '';
            fileInfo.classList.remove('show');
            fileDropArea.classList.remove('has-file');
            
            // Restaurar texto original
            fileDropArea.querySelector('.drop-content').innerHTML = `
                <i class="bi bi-cloud-upload text-primary" style="font-size: 3rem;"></i>
                <h5 class="mt-3 mb-2">Arrastra tu video aqu√≠</h5>
                <p class="text-muted mb-3">o <span class="text-primary" style="cursor: pointer;">haz clic para seleccionar</span></p>
                <small class="text-muted">Formatos soportados: MP4, AVI, MOV, WMV (m√°ximo 2GB)</small>
            `;

            hideStatus();
        }

        function formatFileSize(bytes) {
            if (bytes === 0) return '0 Bytes';
            const k = 1024;
            const sizes = ['Bytes', 'KB', 'MB', 'GB'];
            const i = Math.floor(Math.log(bytes) / Math.log(k));
            return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
        }

        function showStatus(type, message) {
            uploadStatus.className = `upload-status ${type}`;
            uploadStatus.innerHTML = `
                <i class="bi bi-${type === 'success' ? 'check-circle' : 'exclamation-triangle'} me-2"></i>
                ${message}
            `;
        }

        function hideStatus() {
            uploadStatus.className = 'upload-status';
        }

        // Manejar env√≠o del formulario
        uploadForm.addEventListener('submit', async (e) => {
            e.preventDefault();

            if (!fileInput.files[0]) {
                showStatus('error', 'Por favor selecciona un archivo de video.');
                return;
            }

            const formData = new FormData();
            formData.append('video', fileInput.files[0]);
            formData.append('titulo', document.getElementById('titulo').value);
            formData.append('descripcion', document.getElementById('descripcion').value);
            formData.append('id_modulo', document.getElementById('id_modulo').value);
            formData.append('duracion_minutos', document.getElementById('duracion_minutos').value);
            formData.append('estatus', document.getElementById('estatus').value);

            // Mostrar progreso
            uploadProgress.style.display = 'block';
            submitBtn.disabled = true;
            submitBtn.innerHTML = '<i class="bi bi-hourglass-split me-2"></i>Subiendo...';
            hideStatus();

            try {
                const response = await fetch('/videos-admin/upload', {
                    method: 'POST',
                    body: formData
                });

                const result = await response.json();

                if (result.success) {
                    showStatus('success', `¬°Video subido exitosamente! Se ha almacenado en Vimeo: ${result.vimeo_url}`);
                    
                    // Redirigir despu√©s de 3 segundos
                    setTimeout(() => {
                        window.location.href = '/videos-admin';
                    }, 3000);
                } else {
                    showStatus('error', 'Error al subir el video: ' + result.error);
                    submitBtn.disabled = false;
                    submitBtn.innerHTML = '<i class="bi bi-cloud-upload me-2"></i>Subir Video a Vimeo';
                }
            } catch (error) {
                console.error('Error:', error);
                showStatus('error', 'Error de conexi√≥n al subir el video.');
                submitBtn.disabled = false;
                submitBtn.innerHTML = '<i class="bi bi-cloud-upload me-2"></i>Subir Video a Vimeo';
            }

            uploadProgress.style.display = 'none';
        });

        console.log('üìπ Sistema de subida de videos StartEducation iniciado');
    </script>
</body>
</html>